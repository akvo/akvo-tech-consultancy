#remove html file extension prepolling, postpolling, observation
RewriteEngine on 
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]

RewriteCond %{HTTP_HOST} ^wai-eth.tc.akvo.org [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://wai-eth.tc.akvo.org/$1 [R,L]