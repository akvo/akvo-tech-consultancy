{
    "color": [
        "#2f4554",
        "#c23531",
        "#749f83",
        "#d48265"
    ],
    "tooltip": {
        "trigger": "axis",
        "axisPointer": {
            "type": "shadow"
        }
    },
    "legend": {
        "show": true,
        "itemWidth": 50,
        "itemHeight": 20,
        "borderRadius": 100,
        "data": ["Winning Candidate", "Second Place"],
        "padding": 0,
        "textStyle": {
            "fontSize": 24,
            "fontWeight": "bold",
            "margin": 20,
            "width": "100%"
        }
    },
    "grid": {
        "left": 30,
        "right": "5%",
        "bottom": 45,
        "top": 50,
        "containLabel": true
    },
    "xAxis": {
        "type": "category",
        "data": [
            "Batch 1",
            "Batch 2",
            "Batch 3",
            "Batch 4",
            "Batch 5",
            "Batch 6"
        ],
        "axisLabel": {
            "fontSize": 24,
            "fontWeight": "bold",
            "margin": 30,
            "width": "100%"
        }
    },
    "yAxis": {
        "max": 100,
        "type": "value",
        "axisLabel": {
            "fontSize": 24,
            "fontWeight": "bold",
            "formatter": "{value} %",
            "margin": 30,
            "width": "100%"
        }
    },
    "series": [{
            "name": "Winning Candidate",
            "type": "bar",
            "barGap": 0,
            "label": {
                "normal": {
                    "fontSize": 24,
                    "fontWeight": "bold",
                    "show": true,
                    "position": "inside",
                    "formatter": "{c}%"
                }
            },
            "data": [39, 39, 32, 18, 33, 44]
        },

        {
            "name": "Second Place",
            "type": "bar",
            "barGap": 0,
            "label": {
                "normal": {
                    "fontSize": 24,
                    "fontWeight": "bold",
                    "show": true,
                    "position": "inside",
                    "formatter": "{c}%"
                }
            },
            "data": [12, 44, 5, 49, 20, 16]
        }
    ]
}
