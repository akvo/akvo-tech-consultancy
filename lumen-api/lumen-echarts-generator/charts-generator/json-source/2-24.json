{
    "color": ["#2f4554", "#c23531", "#749f83", "#d48265"],
    "tooltip": {
        "trigger": "axis",
        "axisPointer": {
            "type": "shadow"
        }
    },
    "legend": {
        "data": ["Yes", "No", "Not Sure"],
        "padding": 20,
        "textStyle": {
            "fontSize": 20,
            "fontWeight": "bold",
            "rotate": 315,
            "margin": 20,
            "width": "100%"
        }
    },
    "grid": {
        "left": 100,
        "right": "10%",
        "bottom": 50,
        "top": 100,
        "containLabel": true
    },
    "xAxis": {
        "type": "category",
        "data": ["Female","Male"],
        "axisLabel": {
            "fontSize": 18,
            "fontWeight": "bold",
            "rotate": 305,
            "margin": 20,
            "width": "100%"
        }
    },
    "yAxis": {
        "type": "value",
        "axisLabel": {
            "fontSize": 18,
            "fontWeight": "bold",
            "formatter": "{value} %",
            "rotate": 305,
            "margin": 20,
            "width": "100%"
        }
    },
    "series": [{
        "name": "Yes",
        "type": "bar",
        "stack": "total",
        "label": {
            "normal": {
                "fontSize": 20,
                "fontWeight": "bold",
                "show": true,
                "position": "inside",
                "formatter": "{c}%"
            }
        },
        "data": [74, 75]
    }, {
        "name": "No",
        "type": "bar",
        "stack": "total",
        "label": {
            "normal": {
                "fontSize": 20,
                "fontWeight": "bold",
                "show": true,
                "position": "inside",
                "formatter": "{c}%"
            }
        },
        "data": [13, 16]
    }, {
        "name": "Not Sure",
        "type": "bar",
        "stack": "total",
        "label": {
            "normal": {
                "fontSize": 20,
                "fontWeight": "bold",
                "show": true,
                "position": "inside",
                "formatter": "{c}%"
            }
        },
        "data": [13, 9]
    }]
}
